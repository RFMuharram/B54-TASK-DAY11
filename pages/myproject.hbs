    <!-- PROJECT FORM-->
    <div class="header">
      <h1 class="title">Add My Project</h1>
    </div>
    <div class="wrapper-form">
      <form>
        <div>
          <label for="title">Project Name</label>
          <input type="text" id="title" />
        </div>

        <div class="flex">
          <div class="startdate">
            <label for="startdate">Start Date</label>
            <input type="date" id="startdate" />
          </div>
          <div class="enddate">
            <label for="enddate">End Date</label>
            <input type="date" id="enddate" />
          </div>
        </div>

        <div>
          <label for="description">Description</label>
          <textarea id="description"></textarea>
        </div>
        <label>Technologies</label>
        <div class="technologies" id="technologies">
          <div class="checkbox">
            <input class="tech" type="checkbox" id="nodejs" />
            <label for="nodejs">Node Js</label><br />
            <input class="tech" type="checkbox" id="react" />
            <label for="react">React Js</label><br />
          </div>
          <div class="checkbox">
            <input class="tech" type="checkbox" id="nextjs" />
            <label for="nextjs">Next Js</label><br />
            <input class="tech" type="checkbox" id="typescript" />
            <label for="typescript">Typescript</label><br />
          </div>
        </div>
        <div>
          <label for="input-image">Upload Image</label>
          <input type="file" id="input-image" />
          <!-- dibuat div beda antara submit button dan upload image, liat video day 10 1:43 menit -->
          <!-- bisa juga buat ulang -->
          <button type="button" onsubmit="addBlog(event)">
            Submit
          </button>
        </div>
      </form>
    </div>
